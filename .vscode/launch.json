    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Hermes Single mode",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "src/hermes2go",
            "cwd": "examples/",
        },
        {
            "name": "Launch http render service",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "src/renderservice",
        },
        {
            "name": "Launch Hermes Batch BBBeurope",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "src/hermes2go",
            "cwd": "../BBBeurope/",
            "args": [
                "-module", "batch",
                "-concurrent", "1",
                "-logoutput",
                "-batch", "nan_test.txt" ,
            ]
        },
    ]
}